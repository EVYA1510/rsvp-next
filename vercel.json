{
  "rewrites": [
    {
      "source": "/api/submit",
      "destination": "/api/submit-proxy"
    }
  ],
  "headers": [
    {
      "source": "/site.webmanifest",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json; charset=utf-8"
        }
      ]
    }
  ]
}
